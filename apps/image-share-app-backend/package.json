{
  "name": "image-share-app-backend",
  "version": "0.1.0",
  "description": "The backend for the image share app",
  "main": "build/server.js",
  "scripts": {
    "cleanup": "rm -rf .cache && rm -rf ./dist && rm -rf ./node_modules && npm i",
    "build": "npx tsc",
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=./tsconfig.json nodemon -w src src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint ./src",
    "migrate": "node dist/db/migrations/migrate up",
    "migrate:dev": "cross-env NODE_ENV=development ts-node src/db/migrations/migrate up",
    "migrate:dev:down:one": "cross-env NODE_ENV=development ts-node src/db/migrations/migrate down",
    "migrate:dev:down:all": "cross-env NODE_ENV=development ts-node src/db/migrations/migrate down --to 0",
    "create-migration": "ts-node src/db/migrations/migrate create --skip-verify --name",
    "seed:dev": "cross-env NODE_ENV=development ts-node src/db/seeds/seed up",
    "seed:dev:down:one": "cross-env NODE_ENV=development ts-node src/db/seeds/seed down",
    "seed:dev:down:all": "cross-env NODE_ENV=development ts-node src/db/seeds/seed down --to 0",
    "create-seed": "ts-node src/db/seeds/seed create --skip-verify --name",
    "test": "echo 'test successful'",
    "test:clearCache": "jest --clearCache",
    "test:integration": "jest --selectProjects integration --detectOpenHandles"
  },
  "author": "Lukas Aichbauer, MSc <lukas@devopscycle.com> (https://devopscycle.com)",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.39.1",
    "@opentelemetry/sdk-metrics": "^1.15.2",
    "@opentelemetry/sdk-node": "^0.41.2",
    "@opentelemetry/sdk-trace-node": "^1.15.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.23",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@types/umzug": "^2.3.3",
    "@types/uuid": "^8.3.4",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.23.2",
    "socket.io": "^4.5.4",
    "umzug": "^3.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.23",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.1.2",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
